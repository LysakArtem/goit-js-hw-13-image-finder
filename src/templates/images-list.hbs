{{#each this}}
<li class="card-item" >
    <div class="photo-card">
    <img class="img-card" src="{{webformatURL}}" alt="images" width = 310 heigh= 250 />

    <div class="stats">
        <p class="stats-item">
        <i class="material-icons">thumb_up</i>
        {{likes}}
        </p>
        <p class="stats-item">
        <i class="material-icons">visibility</i>
        {{views}}
        </p>
        <p class="stats-item">
        <i class="material-icons">comment</i>
        {{comments}}
        </p>
        <p class="stats-item">
        <i class="material-icons">cloud_download</i>
        {{downloads}}
        </p>
    </div>
    </div>
</li>
{{/each}}